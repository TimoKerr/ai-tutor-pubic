{
  "hosting": {
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/**",
        "run": {
          "serviceId": "aitutor",
          "region": "us-central1"
        }
      }
    ],
    "trailingSlash": false
  }
}